alias setJdk7='export JAVA_HOME=$(/usr/libexec/java_home -v 1.7)'
alias setJdk8='export JAVA_HOME=$(/usr/libexec/java_home -v 1.8)'
alias virtualenv-clean='deactivate ; rm -rf virtualenv ; virtualenv --no-setuptools --prompt=`basename ${PWD}`- virtualenv || virtualenv --setuptools --no-site-packages --prompt=`basename ${PWD}`- virtualenv && hash -r && . virtualenv/bin/activate && python ez_setup.py && hash -r && easy_install pip'
export NVIM_TUI_ENABLE_TRUE_COLOR=1
#export HOMEBREW_GITHUB_API_TOKEN=0
setopt hist_ignore_space
